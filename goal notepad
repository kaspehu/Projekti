create table goal
 (
     id          int         not null
         primary key,
     name        varchar(40) null,
     money       int         null,
     probability int         null
 )
     charset = latin1;
 
 insert into goal (id, name, money, probability)
 values  (1, 'SNAIL SHIELD', 1, 3),
         (2, 'SNAIL ADHESIVE', 2, 7),
         (3, 'GOLDEN BALL', 3, 10),
         (4, 'HEADACHE', 4, 7),
         (5, 'RAGING DIARRHEA', 5, 3);
