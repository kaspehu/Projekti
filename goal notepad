create table goal
 (
     id          int         not null
         primary key,
     name        varchar(40) null,
     money       int         null,
     probability int         null
 )
     charset = latin1;
 
 insert into goal (id, name, money, probability)
 values  (1, 'Golden Ball', 0, 25),
         (2, 'Adhesive', 1, 1),
         (3, 'Snail Adhesive', 2, 1),
         (4, 'Shail Shield', 3, 1),
         (5, 'Raging Diarrhea', 4, 1);
